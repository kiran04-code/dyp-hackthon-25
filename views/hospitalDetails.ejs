#loader{
    height: 100%;
    width: 100%;
    background-color: black;
    position: fixed;
    z-index: 999;
    top: 0;
    display: flex;
    align-items: center;
    justify-content: center;
    transition: all ease 1s;
}
#loader h1{
   color: transparent;
   font-family: futura;
   opacity: 0;
   font-size: 4vw;
   background: linear-gradient(to right, #1552F0, #4d8eff);
   -webkit-background-clip: text;
   position: absolute;
   animation-name: loder;
    animation-duration: 1s;
    animation-delay: 1s;
    animation-timing-function: linear;
   
}
#loader h1:nth-child(2){
    animation-delay: 2s;
}
#loader h1:nth-child(3){
    animation-delay: 3s;
}

@keyframes loder {
    0%{
        opacity: 0;
    }
    10%{
        opacity: 1;
    }
    90%{
        opacity: 1;
    }
    100%{
        opacity: 0;
    }
}
