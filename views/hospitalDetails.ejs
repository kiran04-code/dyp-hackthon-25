    function downloadPDF() {
  if (!hospital) {
    alert("No hospital data available.");
    return;
  }

  const { jsPDF } = window.jspdf;
  const doc = new jsPDF();

  // Set Title
  doc.setFont("helvetica", "bold");
  doc.setFontSize(18);
  doc.text("Hospital Details By Lifeshield.com", 10, 10);

  doc.setFont("helvetica", "normal");
  doc.setFontSize(12);

  let y = 20; 

  function addText(title, value) {
    doc.setFont("helvetica", "bold");
    doc.text(title, 10, y);
    doc.setFont("helvetica", "normal");

    let wrappedText = doc.splitTextToSize(value, 180); 
    doc.text(wrappedText, 10, y + 6);

    y += wrappedText.length * 6 + 4; 
  }

  // Add Hospital Data
  addText("Hospital Name:", hospital.name);
  addText("Contact:", hospital.contact);
  addText("Timings:", hospital.timings);
  addText("Address:", hospital.address);
  addText("Services:", hospital.services);

  
  doc.save(`${hospital.name.replace(/\s+/g, '_')}_details.pdf`);
}

